alias ..='cd ..'
alias b='bash'
alias cdn='cd `find -mindepth 1 -maxdepth 1 -type d | xargs ls -td | head -n 1`'
alias cp='cp -i'
alias d='cd'
alias df='df -h'
alias e='env'
alias g='git'
alias h='history'
alias l='ls'
alias la='ls -a'
alias ll='ls -l'
alias md='mkdir'
alias mv='mv -i'
alias p='pwd'
alias rd='rmdir'
alias rem='#'
alias rf='rm -rf'
alias rm='rm -i'
alias v='vim'
alias vi='vim'
export HISTCONTROL=ignoredups
export HISTSIZE=10000
function ghcl() { git clone https://github.com/$1 $2; }
function ghcl_ssh() { git clone git@github.com:$1 $2; }
function ghcln() { git clone https://github.com/$1 $1; }
function ghcln_ssh() { git clone git@github.com:$1 $1; }
function ghget() { curl -O https://raw.githubusercontent.com/$1; }
function mf() { :> $1; }
function sorting() { sort -o $1 $1; }
