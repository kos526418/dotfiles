set ambiwidth=double
set autoindent
set backspace=indent,eol,start
set backup
set backupdir=~/.cache/vim/backups
set directory=~/.cache/vim/swaps
set encoding=utf-8
set expandtab
set fileformats=unix,dos,mac
set hidden
set list
set listchars=eol:^,nbsp:%,tab:>=,trail:-
set number
set shiftwidth=2
set smartindent
set softtabstop=2
set swapfile
set tabstop=2
set title
set undodir=~/.cache/vim/undos
set viminfo+=n~/.cache/vim/viminfo
set virtualedit=block
set wildmenu
